{
  "name": "describe-module",
  "version": "0.0.1",
  "description": "A tiny library to help you find out where a given node module has been installed",
  "scripts": {
    "test": "node ./node_modules/gulp/bin/gulp.js test",
    "prepublish": "node ./node_modules/gulp/bin/gulp.js prepublish",
    "postinstall": "npm run typings -- install",
    "typings": "typings",
    "report-coverage": "cat ./staging/reports/coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/jan-molak/describe-module.git"
  },
  "bugs": {
    "url": "https://github.com/jan-molak/describe-module/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "homepage": "https://github.com/jan-molak/describe-module",
  "license": "Apache-2.0",
  "dependencies": {
    "global-modules": "0.2.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.12",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-concat": "2.6.0",
    "gulp-istanbul": "1.0.0",
    "gulp-mocha": "3.0.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "6.0.2",
    "gulp-typescript": "2.13.6",
    "merge2": "1.0.2",
    "mocha": "3.0.0",
    "mock-fs": "3.11.0",
    "remap-istanbul": "0.6.4",
    "run-sequence": "1.2.2",
    "ts-helper": "0.0.1",
    "ts-node": "1.2.2",
    "tslint": "3.14.0",
    "typescript": "1.8.10",
    "typings": "1.3.2"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 2"
  }
}
